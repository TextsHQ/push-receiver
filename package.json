  },
  "devDependencies": {
    "@textshq/eslint-config": "https://github.com/TextsHQ/eslint-config#main",
    "@types/node": "^17.0.35",
    "eslint": "^8.0.0",
    "http-proxy": "^1.16.2",
    "husky": "^0.14.3",
    "jest": "^22.2.2",
    "typescript": "^4.7.2",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "long": "^3.2.0",
    "protobufjs": "^6.8.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "uuid": "^3.1.0"
  }
