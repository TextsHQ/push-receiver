{
  "name": "push-receiver",
  "version": "3.0.0",
  "description": "A module to subscribe to GCM/FCM and receive notifications within a node process.",
  "main": "dist/client.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node scripts/listen",
    "register": "node scripts/register",
    "send": "node scripts/send",
